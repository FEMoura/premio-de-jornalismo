<section class="section-title registration">
  <div class="container">
    <%= simple_form_for [:sindjornal, @journalist] do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :name %>
        <%= f.input :cpf %>
      </div>

      <div class="form-actions">
        <%= f.button :submit, 'Criar' %>
      </div>
    <% end %>
</div>
</section>